    "new_planet = input(\"Ingresa el nombre de un planeta\")\n",
    "planets = []\n",
    "\n",
    "while new_planet.lower() != 'done':\n",
    "    if new_planet:\n",
    "        planets.append(new_planet)\n",
    "    new_planet = input(\"Ingresa un planeta nuevo\")\n",
    "\n",
    "\n",
    "for planet in planets:\n",
    "    print(planet)\n",
    "\n"
